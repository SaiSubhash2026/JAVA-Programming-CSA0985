 class GCD  
{  
    public static void main(String[] args)    
    {
        int num1=2,num2=7;
       while(num1 != num2) 
        {
          if(num1 > num2) 
          {
            num1 -= num2;
          }
          else 
          {
            num2 -= num1;
          }
        }
      System.out.println("GCD of given two numbers is :"+num1);
   }  
}  
